@{
    ViewData["Title"] = "Mod";
}

<html>
<head>
    <meta charset="utf-8" />
    <title>Abilities</title>
</head>
<body>
    <link rel="stylesheet" href="~/StyleSheet.css"/>
    <p class="title">
        @ViewData["Mod Name"]
    </p>
    <table>
        <tr>
            <td class="prop">
                Rank
            </td>
            <td class="prop">
                Drain
            </td>
            <td class="prop">
                Effect
            </td>
        </tr>
        @foreach (var rank in ViewData["Ranks"] as List<Warframe.Models.Mod>)
        {
            <tr>
                <td class="prop">
                    @rank.GetRank()
                </td>
                <td class="prop">
                    @rank.GetDrain()
                </td>
                <td class="prop">
                    @rank.GetEffect()
                </td>
            </tr>
        }
    </table>
</body>
</html>